--// SETTINGS \\--
repeat wait() until game:IsLoaded()
_G.KEY = "q" -- Key to use aimlock (ANY BUT SMALL LETTER ONLY)
_G.PART = "HumanoidRootPart" -- What part to aim at
_G.PRED = 0.36; -- Prediction
_G.Frame = Vector3.new(0,0.53,0) -- Frame
--// SCRIPT \\--
local a=game:GetService"Workspace".CurrentCamera;local b;local c=false;local d=nil;local e=game.Players.LocalPlayer:GetMouse()local f=Instance.new("Part",game.Workspace)local g=Instance.new("Folder",game.CoreGui)function makemarker(h,i,j,k,l)local m=Instance.new("BillboardGui",h)m.Name="PP"m.Adornee=i;m.Size=UDim2.new(k,l,k,l)m.AlwaysOnTop=true;local n=Instance.new("Frame",m)n.Size=UDim2.new(4,0,4,0)n.BackgroundTransparency=0.1;n.BackgroundColor3=j;local o=Instance.new("UICorner",n)o.CornerRadius=UDim.new(50,50)return m end;local p=game.Players:GetPlayers()function noob(q)local r;repeat wait()until q.Character;local s=makemarker(g,q.Character:WaitForChild(_G.PART),Color3.fromRGB(255,255,255),0.0,0)s.Name=q.Name;q.CharacterAdded:connect(function(t)s.Adornee=t:WaitForChild(_G.PART)end)local u=Instance.new("TextLabel",s)u.BackgroundTransparency=1;u.Position=UDim2.new(0,0,0,-50)u.Size=UDim2.new(0,100,0,100)u.Font=Enum.Font.SourceSansSemibold;u.TextSize=14;u.TextColor3=Color3.new(1,1,1)u.TextStrokeTransparency=0;u.TextYAlignment=Enum.TextYAlignment.Bottom;u.Text="Bling"..q.Name;u.ZIndex=10;spawn(function()while wait()do if q.Character then end end end)end;for v=1,#p do if p[v]~=game.Players.LocalPlayer then noob(p[v])end end;game.Players.PlayerAdded:connect(function(w)noob(w)end)game.Players.PlayerRemoving:Connect(function(x)g[x.Name]:Destroy()end)spawn(function()f.Anchored=true;f.CanCollide=false;f.Size=Vector3.new(0.1,0.1,0.1)f.Transparency=0.1;makemarker(f,f,Color3.fromRGB(255,255,255),0.20,0)end)e.KeyDown:Connect(function(y)if y~=_G.KEY then return end;if c then c=false;TextLabel.TextColor3=Color3.fromRGB(255,20,75)TextLabel.Text="------"else c=true;b=getClosestPlayerToCursor()TextLabel.TextColor3=Color3.fromRGB(12,255,0)TextLabel.Text=b.Character.Humanoid.DisplayName end end)function getClosestPlayerToCursor()local z;local A=math.huge;for B,C in pairs(game.Players:GetPlayers())do if C~=game.Players.LocalPlayer and C.Character and C.Character:FindFirstChild("Humanoid")and C.Character.Humanoid.Health~=0 and C.Character:FindFirstChild(_G.PART)then local D=a:WorldToViewportPoint(C.Character.PrimaryPart.Position)local E=(Vector2.new(D.X,D.Y)-Vector2.new(e.X,e.Y)).magnitude;if E<A then z=C;A=E end end end;return z end;game:GetService"RunService".Stepped:connect(function()if c and b.Character and b.Character:FindFirstChild(_G.PART)then f.CFrame=CFrame.new(b.Character[_G.PART].Position+_G.Frame+b.Character[_G.PART].Velocity*d)else f.CFrame=CFrame.new(0,9999,0)end end)local F=getrawmetatable(game)local G=F.__namecall;setreadonly(F,false)F.__namecall=newcclosure(function(...)local H={...}if c and getnamecallmethod()=="FireServer"and H[2]=="MousePos"then H[3]=b.Character[_G.PART].Position+_G.Frame+b.Character[_G.PART].Velocity*d;return G(unpack(H))end;return G(...)end)game.Players.LocalPlayer.Chatted:Connect(function(I)if I=="/e print"then print(_G.PRED)end end)game.Players.LocalPlayer.Chatted:Connect(function(J)if J=="Code:1029"then _G.KEY=nil;_G.AIR=nil;_G.PART=nil;_G.PRED=nil;TextLabel.Visible=false end end)game.Players.LocalPlayer.Chatted:Connect(function(K)if K=="/e hrp"then _G.KEY="q"_G.AIR=0.00005;_G.PART="HumanoidRootPart"_G.PRED=0.032;TextLabel.Visible=true end end)game.Players.LocalPlayer.Chatted:Connect(function(L)if L=="/e lt"then _G.KEY="q"_G.AIR=0.00005;_G.PART="LowerTorso"_G.PRED=0.032;TextLabel.Visible=true end end)game.Players.LocalPlayer.Chatted:Connect(function(L)if L=="/e hd"then _G.KEY="q"_G.AIR=0.00005;_G.PART="Head"_G.PRED=0.032;TextLabel.Visible=true end end)game.Players.LocalPlayer.Chatted:Connect(function(M)if M=="Screensharing"then _G.KEY="q"_G.AIR=0.00005;_G.PART="LowerTorso"_G.PRED=0.033;TextLabel.Visible=true;f=nil end end)game.Players.LocalPlayer.Chatted:Connect(function(N)if N=="/e P+"then _G.PRED=_G.PRED+0.001 end end)game.Players.LocalPlayer.Chatted:Connect(function(O)if O=="/e P-"then _G.PRED=_G.PRED-0.001 end end)while wait()do if getClosestPlayerToCursor().Character.Humanoid.Jump==true and getClosestPlayerToCursor().Character.Humanoid.FloorMaterial==Enum.Material.Air then _G.Frame=Vector3.new(0,-2.3,0)wait(0.05)else local P=game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()local Q=tostring(P)local R=Q:split(" ")local S=R[1]d=S/1000+_G.PRED;_G.Frame=Vector3.new(0,0.53,0)end end
